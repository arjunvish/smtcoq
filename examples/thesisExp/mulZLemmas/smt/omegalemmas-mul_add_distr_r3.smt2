(set-option :print-success true)
(set-option :produce-assignments true)
(set-option :produce-abducts true)
(set-option :incremental true)
(set-logic QF_UFLIA)
(set-option :sygus-core-connective false)
(declare-fun l1 () Int)
(declare-fun k2 () Int)
(declare-fun l2 () Int)
(declare-fun c2 () Int)
(declare-fun mul (Int Int ) Int)
(declare-fun v2 () Int)
(assert (= (mul v2 (mul c2 k2)) (mul (mul v2 c2) k2)))
(get-abduct A (= (+ l1 (mul (+ (mul v2 c2) l2) k2)) (+ (mul v2 (mul c2 k2)) (+ l1 (mul l2 k2)))))

