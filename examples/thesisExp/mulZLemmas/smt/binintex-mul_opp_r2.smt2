(declare-fun mul (Int Int ) Int)
(declare-fun b () Int)
(assert (= (mul b 1) b))
(assert (= (+ (- b) b) 0))
(get-abduct A (= (- r) (+ (+ (mul b (- 1)) b) (- r))))
