
y, ~b	~(x = y),  x, ~y
-----------------------
	~b, ~(x = y), x	a = b, a, b
	-----------------------------------
		~(x = y), a = b, x, a		(x = y) = (a = b), x = y, a = b
		--------------------------------------------------------------
				(x = y) = (a = b), a = b, x, a

If y = a,


If y = b, 
~(x = y),  x, ~y	a = b, a, b
----------------------------------
	~(x = y),  x, a = b, a		(x = y) = (a = b), x = y, a = b
	--------------------------------------------------------------
			(x = y) = (a = b), a = b, x, a

If y = a && y = b
~(x = y),  x, ~y	a = b, a, b
----------------------------------
	~(x = y),  x, a = b		(x = y) = (a = b), x = y, a = b
	--------------------------------------------------------------
			(x = y) = (a = b), a = b, x
			
		
		
(x = y) = (y = y)

-------------------------------------		-------
(x = y) = (y = y), ~(x = y), ~(y = y)		y = y
---------------------------------------------------
	(x = y) = (y = y), ~(x = y)
